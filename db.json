  "formations": [
    {
      "id": 1,
      "titre": "Formation sur l'utilisation de la machine X",
      "description": "Cette formation vise à familiariser les employés avec la machine X et à leur permettre de maîtriser son fonctionnement, ses réglages et ses procédures de maintenance."
    },
    {
      "id": 2,
      "titre": "Formation sur les procédures de sécurité",
      "description": "Cette formation vise à sensibiliser les employés aux procédures de sécurité en vigueur dans l'entreprise et à les former aux bonnes pratiques à adopter lors de l'utilisation des machines."
    },
    {
      "id": 3,
      "titre": "Formation avancée sur la maintenance préventive",
      "description": "Cette formation s'adresse aux techniciens de maintenance et vise à approfondir leurs connaissances et compétences en matière de maintenance préventive des machines industrielles."
    },
    {
      "id": 4,
      "titre": "Formation sur les techniques d'assemblage",
      "description": "Cette formation permet aux employés d'acquérir les compétences nécessaires pour effectuer des assemblages complexes en suivant des procédures précises."
    },
    {
      "id": 5,
      "titre": "Formation sur la programmation des machines",
      "description": "Cette formation enseigne aux participants les bases de la programmation des machines industrielles, leur permettant ainsi de créer des programmes personnalisés pour des tâches spécifiques."
    },
    {
      "id": 6,
      "titre": "Formation sur la gestion de la qualité",
      "description": "Cette formation se concentre sur les principes et les techniques de gestion de la qualité dans le contexte d'une entreprise industrielle, visant à améliorer la satisfaction du client et la performance globale."
    },
    {
      "id": 7,
      "titre": "Formation sur la sécurité au travail",
      "description": "Cette formation sensibilise les employés aux risques potentiels sur le lieu de travail et leur enseigne les mesures de sécurité nécessaires pour prévenir les accidents et les blessures."
    },
    {
      "id": 8,
      "titre": "Formation sur la logistique et la gestion des stocks",
      "description": "Cette formation aborde les aspects clés de la logistique et de la gestion des stocks, fournissant aux participants les connaissances nécessaires pour optimiser les opérations et minimiser les coûts."
    },
    {
      "id": 9,
      "titre": "Formation sur l'ergonomie au travail",
      "description": "Cette formation met l'accent sur l'importance de l'ergonomie dans l'environnement de travail, en fournissant des conseils pratiques pour améliorer le confort, la sécurité et l'efficacité des employés."
    },
    {
      "id": 10,
      "titre": "Formation sur la maintenance des équipements",
      "description": "Cette formation fournit aux techniciens les compétences nécessaires pour entretenir et réparer divers équipements industriels, en minimisant les temps d'arrêt"
    }
  ],
